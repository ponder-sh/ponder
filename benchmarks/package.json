    "@ponder/core": "workspace:*",
    "@types/node": "^20.9.0",
    "execa": "^8.0.1",
    "parse-prometheus-text-format": "^1.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "viem": "1.19.1"
  }
}
